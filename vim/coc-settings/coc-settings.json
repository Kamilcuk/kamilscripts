{
	"java.jdt.ls.home": "/usr/share/java/jdtls",
	"https.rejectUnauthorized": false,
	"http.proxyStrictSSL": false,
  "java.home": "/usr/lib/jvm/java-11-openjdk",
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.formatOnType": false,
  "coc.preferences.formatOnSaveFiletypes": [],
  "coc.preferences.semanticTokensHighlights": false,
  "coc.source.file.triggerCharacters": [
    "/"
  ],
  "diagnostic.errorSign": "âœ˜",
  "diagnostic.warningSign": "âš ",
  "diagnostic.infoSign": "ðŸ›ˆ",
  "diagnostic.hintSign": "â˜¡",
  "diagnostic.displayByAle": false,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.checkCurrentLine": false,
  "diagnostic.virtualTextPrefix": " â‡’â‡’â‡’ ",
  "diagnostic.virtualText": true,
  "suggest.minTriggerInputLength": 3,
  "suggest.triggerCompletionWait": 50,
  "codeLens.enable": true,
  "solargraph.checkGemVersion": false,
  "clangd.semanticHighlighting": true,
  "clangd.enabled": true,
  "clangd.arguments": [
    "--background-index",
    "--clang-tidy",
    "-j=4",
    "--completion-style=detailed",
    "--pch-storage=memory"
  ],
  "coverage.enabled": true,
  "coverage.jsonReportPath": "./coverage/coverage-final.json",
  "python.linting.pylintEnabled": false,
  "python.linting.enabled": true,
  "python.jediEnabled": true,
  "python.jediPath": "/usr/bin/jedi-language-server",
  "diagnostic-languageserver.enable": true,
  "diagnostic-languageserver.mergeConfig": true,
  "diagnostic-languageserver.debug": false,
  "diagnostic-languageserver.trace.server": "off",
  "diagnostic-languageserver.filetypes": {
    "c": [
      "cpplint_c",
      "gcovrtolist"
    ],
    "cpp": [
      "cpplint_cpp",
      "gcovrtolist"
    ],
    "vim": "vint",
    "email": "languagetool",
    "markdown": [
      "write-good",
      "markdownlint"
    ],
    "sh": "shellcheck",
    "elixir": [
      "mix_credo",
      "mix_credo_compile"
    ],
    "eelixir": [
      "mix_credo",
      "mix_credo_compile"
    ],
    "php": [
      "phpstan",
      "psalm"
    ],
    "yaml": [
      "yamllint",
      "ansible-lint2"
    ],
    "cmake": [
      "cmake-lint",
      "cmakelint"
    ],
    "python": [
      "pylava"
    ],
    "systemd": "systemd-analyze",
    "PKGBUILD": "namcap",
    "_comment_make": "checkmake",
    "hcl": "nomad_validate"
  },
  "diagnostic-languageserver.linters": {
    "nomad_validate": {
      "sourceName": "nomad_validate",
      "command": "sh",
      "args": [
        "-c",
        "if hash nomad 2>/dev/null && test \"${1##*.}\" == nomad; then nomad validate \"$1\" 2>&1; fi",
        "_",
        "%file"
      ],
      "offsetLine": 1,
      "formatPattern": [
        "^\\s+\\* (.+)$",
        {
          "message": 1
        }
      ]
    },
    "cpplint_c": {
      "args": [
        "--filter=-whitespace,-legal/copyright,-runtime/int,-build/include_subdir,-readability/casting,-build/include_order,-readability/todo,-build/include,-readability/nolint,-readability/braces",
        "%file"
      ],
      "command": "cpplint",
      "debounce": 100,
      "isStderr": true,
      "isStdout": false,
      "offsetLine": 0,
      "offsetColumn": 0,
      "formatPattern": [
        "^[^:]+:(0:|(\\d+):)(\\d+)?\\s+(.+?)\\s\\[(\\d)\\]$",
        {
          "line": 2,
          "column": 3,
          "message": 4,
          "security": 5
        }
      ],
      "securities": {
        "1": "info",
        "2": "warning",
        "3": "warning",
        "4": "error",
        "5": "error"
      }
    },
    "cpplint_cpp": {
      "args": [
        "--filter=-whitespace,-legal/copyright,-runtime/int,-build/include_subdir,-readability/casting,-build/include_order,-readability/todo,-build/include,-readability/nolint",
        "%file"
      ],
      "command": "cpplint",
      "debounce": 100,
      "isStderr": true,
      "isStdout": false,
      "offsetLine": 0,
      "offsetColumn": 0,
      "formatPattern": [
        "^[^:]+:(0:|(\\d+):)(\\d+)?\\s+(.+?)\\s\\[(\\d)\\]$",
        {
          "line": 2,
          "column": 3,
          "message": 4,
          "security": 5
        }
      ],
      "securities": {
        "1": "info",
        "2": "warning",
        "3": "warning",
        "4": "error",
        "5": "error"
      }
    },
    "gcovrtolist": {
      "sourceName": ",gcovr-to-list",
      "command": ",gcovr-to-list",
      "args": [
        "-f",
        "%filepath",
        "./coverage.json"
      ],
      "rootPatterns": [
        "coverage.json"
      ],
      "offsetColumn": 1,
      "formatPattern": [
        "^(\\d+)(:)(\\d+): (.*)$",
        {
          "line": 1,
          "column": 3,
          "security": 2,
          "message": 4
        }
      ],
      "securities": {
        ":": "hint"
      }
    },
    "namcap": {
      "sourceName": "namcap",
      "command": "namcap",
      "args": [
        "%filepath"
      ],
      "formatPattern": [
        "^[^ ]* (\\([^\\)]*\\) ([^:]*): .*)$",
        {
          "security": 2,
          "message": 1
        }
      ],
      "securities": {
        "I": "hint",
        "R": "info",
        "C": "info",
        "D": "info",
        "W": "warning",
        "E": "error",
        "F": "error"
      }
    },
    "pylava": {
      "sourceName": "pylava",
      "command": "pylava",
      "args": [
        "-F",
        "-fparsable",
        "-lmccabe,pycodestyle,pyflakes",
        "%filepath"
      ],
      "formatPattern": [
        "^[^:]*:(\\d+?):(\\d+?): \\[(.)\\] (.*)$",
        {
          "line": 1,
          "column": 2,
          "security": 3,
          "message": 4
        }
      ],
      "securities": {
        "I": "hint",
        "R": "info",
        "C": "info",
        "D": "info",
        "W": "warning",
        "E": "error",
        "F": "error"
      }
    },
    "checkmake": {
      "sourceName": "checkmake",
      "command": "sh",
      "_comment": "checkmake output CR characters and 0 lines. Filter them and fix",
      "args": [
        "-c",
        "checkmake --format={{.LineNumber}}@{{.Rule}}:{{.Violation}} \"$0\" | sed 's/^0@/1@/; s/\\r//g'",
        "%filepath"
      ],
      "formatPattern": [
        "^(\\d+)@(.*)$",
        {
          "line": 1,
          "message": 2
        }
      ]
    },
    "yamllint": {
      "args": [
        "-d",
        "{extends: relaxed, rules: { line-length: {max: 120} }, truthy: { allowed-values: [\"yes\", \"true\", \"no\", \"false\"] } }",
        "-f",
        "parsable",
        "-"
      ]
    },
    "shellcheck": {
      "args": [
        "-x",
        "--format",
        "json1",
        "-"
      ]
    },
    "systemd-analyze": {
      "command": "systemd-analyze",
      "debounce": 100,
      "args": [
        "verify",
        "%filepath"
      ],
      "isStdout": false,
      "isStderr": true,
      "sourceName": "systemd-analyze",
      "formatLines": 1,
      "formatPattern": [
        "^[^:]+:((\\d+):)?\\s*(.*)$",
        {
          "line": 2,
          "message": 3
        }
      ]
    },
    "cmakelint": {
      "command": "cmakelint",
      "debounce": 100,
      "args": [
        "--linelength=120",
        "--filter=-whitespace/tabs",
        "%filepath"
      ],
      "offsetLine": 0,
      "offsetColumn": 1,
      "sourceName": "cmakelint",
      "formatLines": 1,
      "formatPattern": [
        "^[^:]+:(\\d+): (.*)$",
        {
          "line": 1,
          "message": 2
        }
      ]
    },
    "cmake-lint": {
      "command": "cmake-lint",
      "debounce": 100,
      "args": [
        "--line-width=120",
        "--use-tabchars=1",
        "%filepath"
      ],
      "offsetLine": 0,
      "offsetColumn": 1,
      "sourceName": "cmake-lint",
      "formatLines": 1,
      "formatPattern": [
        "^[^:]+:(\\d+)(,(\\d+))?: (\\[(.).*)$",
        {
          "line": 1,
          "column": 3,
          "message": 4,
          "security": 5
        }
      ],
      "securities": {
        "C": "info",
        "R": "info",
        "W": "warning",
        "E": "error"
      }
    },
    "ansible-lint": {
      "command": "ansible-lint",
      "sourceName": "ansible-lint",
      "args": [
        "--nocolor",
        "--parseable-severity",
        "--offline",
        "-p",
        "%file"
      ],
      "rootPatterns": [
        "ansible.cfg",
        ".git"
      ],
      "isStdout": true,
      "isStderr": false,
      "formatPattern": [
        "^([^:]+):(\\d+):(\\d+):(\\s*\\S+\\s*\\[(\\S+?)\\].*)$",
        {
          "sourceName": 1,
          "sourceNameFilter": true,
          "line": 2,
          "column": 3,
          "security": 5,
          "message": 4
        }
      ],
      "securities": {
        "VERY_HIGH": "error",
        "HIGH": "warning",
        "MEDIUM": "warning",
        "LOW": "info",
        "VERY_LOW": "hint",
        "INFO": "hint"
      }
    }
  },
  "diagnostic-languageserver.formatFiletypes": {
    "dart": "dartfmt",
    "elixir": "mix_format",
    "eelixir": "mix_format",
    "python": [
      "black"
    ],
    "lua": "lua-format",
    "sh": "shfmt",
    "blade": "blade-formatter",
    "cmake": "cmake-format"
  },
  "diagnostic-languageserver.formatters": {
    "shfmt": {
      "command": "shfmt",
      "args": [
        "-i",
        "2",
        "-bn",
        "-ci",
        "-sr"
      ]
    },
    "cmake-format": {
      "command": "cmake-format",
      "args": [
        "-"
      ]
    }
  },
  "yaml.customTags": [
    "!reference sequence"
  ]
}
